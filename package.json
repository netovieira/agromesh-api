    "typescript": "^4.3.5",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.0.7",
    "@adonisjs/core": "^5.1.11",
    "@adonisjs/lucid": "^15.0.3",
    "@adonisjs/repl": "^3.1.5",
    "@types/socket.io": "^3.0.2",
    "adonis5-scheduler": "^2.0.2",
    "axios": "^0.21.1",
    "firebase-admin": "^9.11.1",
    "luxon": "^2.0.1",
    "mysql": "^2.18.1",
    "phc-argon2": "^1.1.2",
    "phc-bcrypt": "^1.0.7",
